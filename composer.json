{
  "name": "divanteltd/coreshop-vsbridge",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "coreshop-vsbridge",
  "keywords": [
    "pimcore",
    "pimcore-plugin"
  ],
  "homepage": "https://divante.co",
  "autoload": {
    "psr-4": {
      "CoreShop2VueStorefrontBundle\\": "src/"
    }
  },
  "require": {
    "php": ">=7.1",
    "cocur/slugify": "^3.1",
    "coreshop/core-shop": "dev-master",
    "gesdinet/jwt-refresh-token-bundle": "^0.5.3",
    "gfreeau/get-jwt-bundle": "2.0.x-dev",
    "ongr/elasticsearch-bundle": "dev-master",
    "lexik/jwt-authentication-bundle": "^2.5",
    "mockery/mockery": "^1.1",
    "nelmio/cors-bundle": "^1.5",
    "pimcore/pimcore": "~5.4.0",
    "symfony-bundles/json-request-bundle": "^2.0",
    "wikimedia/composer-merge-plugin": "^1.4",
    "divanteltd/pimcoredevkit": "^0.6.6"
  },
  "require-dev": {
    "cache/integration-tests": "^0.15.0",
    "codeception/codeception": "~2.3.8"
  },
  "minimum-stability": "dev",
  "extra": {
    "pimcore": {
      "bundles": [
        "CoreShop2VueStorefrontBundle\\CoreShop2VueStorefrontBundle"
      ]
    }
  },
  "scripts": {
    "post-package-install": [
      "php bin/console pimcore:bundle:enable PimcoreDevkitBundle --no-interaction"
    ]
  }
}
